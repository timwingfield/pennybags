<div class="jumbotron">
  <p class="lead">View Properties owned by the Bank</p>
  <div class="row">
    <div class="span4 property-details">
      <div class="property-title well dark_blue"></div>
      <h3>Boardwalk</h3>
      <p><strong>Owner:</strong> Bank</p>
      <p><strong>Rent:</strong> $400</p>
      <p><strong>Mortgage Value:</strong> $400</p>
    </div>
  </div>
</div>

<hr>

<div class="row property-list">
</div>

<script type="text/javascript">
  $(function(){
    $('.test').text("Adding some text");    

    $.get('/properties', function(data) { 
      $.each(data, function(){
        html = '<div class="small-card span2">';
        html += '<div class="small-property-card ' + this.property_group + '"></div>';
        html += '<h4>' + this.name + '</h4>';
        html += '</div>'
        $('.property-list').append(html);
      });
    }); 

  });
</script>
